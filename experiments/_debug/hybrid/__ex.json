{
  "out_path": "./experiments/_debug/hybrid/",
  "model": {
    "encoding": {
      "model": "fabriceyhc/bert-base-uncased-imdb",
      "layers": [
        -1
      ]
    },
    "base": {
      "hid_size": 256,
      "dropout": 0.2
    },
    "features": {
      "dropout": 0.1,
      "ngram_counter": {
        "1": 1024,
        "2": 128
      },
      "nela_pipeline": {
        "style": true,
        "complexity": true,
        "bias": true,
        "affect": true,
        "moral": true,
        "event": true
      }
    },
    "ensemble": {
      "size": 32,
      "dropout": 0.1
    }
  }
}