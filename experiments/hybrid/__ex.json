{
  "out_path": "./experiments/hybrid/",
  "model": {
    "encoding": {
      "model": "fabriceyhc/bert-base-uncased-imdb",
      "layers": [
        -1
      ]
    },
    "base": {
      "hid_size": 256,
      "dropout": 0.4
    },
    "features": {
      "ngram_counter": {
        "1": 768,
        "2": 5120
      },
      "spacy_pipeline": {
        "model": "en_core_web_sm",
        "pos_tags": [
          "NOUN",
          "VERB",
          "ADV",
          "ADJ",
          "INTJ",
          "SYM"
        ],
        "ents": true,
        "blob": true,
        "disabled": []
      }
    }
  }
}
